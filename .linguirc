{
    "locales": ["en", "cs", "ru", "uk"],
    "fallbackLocales": {
        "default": "en"
        },
    "catalogs": [
        {
          "path": "src/locales/{locale}/messages",
          "include": [
            "src"
          ],
          "exclude": [
            "src/locales/**"
          ]
        }
      ],
    "format": "po",
    "compileNamespace": "es",
    "sourceLocale": "en",
    "extractBabelOptions": {
    "presets": [
      "@babel/preset-typescript"
    ]
  }
}
